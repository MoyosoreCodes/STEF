<body>
    <div id="wrapper">
        <nav class="navbar navbar-dark align-items-start sidebar sidebar-dark accordion bg-gradient-primary p-0" style="background: #800080;margin-top: 0px;">
            <div class="container-fluid d-flex flex-column p-0"><a class="navbar-brand" href="#" style="font-family: Adamina, serif;font-size: 60px;margin-top: 68px;">CUCC</a>
                <hr class="sidebar-divider my-0">
                <ul class="navbar-nav text-light" id="accordionSidebar-1" style="height: 502px;margin-top: 44px;">
                    <li class="nav-item" style="height: 60px;font-size: 16px;"><a class="nav-link" href="/dashboard/counsellor" style="font-size: 20px;"><i class="fas fa-tachometer-alt"></i><span>Dashboard</span></a></li>
                    <li class="nav-item" style="height: 60px;"><a class="nav-link" href="/dashboard/profile" style="font-size: 20px;"><i class="fas fa-user"></i><span>Profile</span></a></li>
                    <li class="nav-item" style="height: 214px;"><a class="nav-link" href="/dashboard/sessions" style="font-size: 20px;"><i class="fas fa-table"></i><span>Sessions</span></a>
                        <a class="nav-link" href="/dashboard/users" style="font-size: 20px;"><i class="fa fa-sticky-note"></i>Users</a>
                        <a class="nav-link active" href="/dashboard/appointments" style="font-size: 20px;"><i class="fas fa-clipboard"></i>Appointment</a></li>
                    <li class="nav-item" style="font-size: 20px;"><a class="nav-link" href="#" style="font-size: 16.6px;"><i class="far fa-user-circle"></i><span>Log out</span></a></li>
                    <li class="nav-item"></li>
                </ul>
                <div class="text-center d-none d-md-inline" style="color: rgb(10,23,122);"><button class="btn rounded-circle border-0" id="sidebarToggle-1" type="button"></button></div>
            </div>
        </nav>
        <div class="d-flex flex-column" id="content-wrapper">
            <div id="content">
                <nav class="navbar navbar-light navbar-expand bg-white shadow mb-4 topbar static-top">
                    <div class="container-fluid"><button class="btn btn-link d-md-none rounded-circle me-3" id="sidebarToggleTop-1" type="button"><i class="fas fa-bars"></i></button>
                        <div img=""><img src="/Counsellor/assets/img/logo.png" style="width: 130px;height: 71.2px;"></div>
                        <ul class="navbar-nav flex-nowrap ms-auto">
                            <div class="d-none d-sm-block topbar-divider"></div>
                            <li class="nav-item dropdown no-arrow">
                                <div class="nav-item dropdown no-arrow"><a class="dropdown-toggle nav-link" aria-expanded="false" data-bs-toggle="dropdown" href="#"><span class="d-none d-lg-inline me-2 text-gray-600 small">Shola Coker</span><img class="border rounded-circle img-profile" src="/Counsellor/assets/img/avatars/avatar1.jpeg"></a>
                                    <div class="dropdown-menu shadow dropdown-menu-end animated--grow-in"><a class="dropdown-item" href="/dashboard/profile2"><i class="fas fa-user fa-sm fa-fw me-2 text-gray-400"></i>&nbsp;Profile</a><a class="dropdown-item" href="#"><i class="fas fa-cogs fa-sm fa-fw me-2 text-gray-400"></i>&nbsp;Settings</a><a class="dropdown-item" href="#"><i class="fas fa-list fa-sm fa-fw me-2 text-gray-400"></i>&nbsp;Activity log</a>
                                        <div class="dropdown-divider"></div><a class="dropdown-item" href="#"><i class="fas fa-sign-out-alt fa-sm fa-fw me-2 text-gray-400"></i>&nbsp;Logout</a>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </nav>
                <div class="container-fluid">
                    <h3 class="text-dark mb-4">Counselling Sessions<a class="btn btn-primary btn-sm d-block float-end d-sm-inline-block" role="button" style="background: #9932CC;" href="/dashboard/session"><i class="fas fa-notes-medical fa-sm text-white-50"></i>&nbsp;Create new sessions</a></h3>
                    <div class="card shadow">
                        <div class="card-header py-3">
                            <p class="text-primary m-0 fw-bold">User's Info</p>
                        </div>
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-6 text-nowrap">
                                    <div id="dataTable_length" class="dataTables_length" aria-controls="dataTable"><label class="form-label">Show&nbsp;<select class="d-inline-block form-select form-select-sm">
                                                <option value="10" selected="">10</option>
                                                <option value="25">25</option>
                                                <option value="50">50</option>
                                                <option value="100">100</option>
                                            </select>&nbsp;</label></div>
                                </div>
                            </div>
                            <div class="table-responsive table mt-2" id="dataTable-1" role="grid" aria-describedby="dataTable_info">
                                <% if(patients) { %>
                                <table class="table table-hover my-0" id="dataTable">
                                    <thead>
                                        <tr class="table-active">
                                            <th>Name</th>
                                            <th>Position</th>
                                            <th>Level/Status</th>
                                            <th>Actions</th>
                                        </tr>
                                    </thead>
                                    <% for(var patient of patients) { %>
                                    <tbody>
                                        <tr>
                                            <td><img class="rounded-circle me-2" width="30" height="30" src="/Counsellor/assets/img/avatars/avatar1.jpeg"><%patient.first_name%> <%patient.last_name%></td>
                                            <td><%=patient.user_type.toLowerCase%></td>
                                            <td><%=patient.level%><br></td>
                                            <td><a class="btn btn-success" role="button" style="width: 75.5px;color: rgb(255,253,253);" href="/dashboard/view/<%=patient._id%>">View</a><a class="btn btn-secondary" role="button" style="margin: 2px;margin-left: 20px;" href="/dashboard/update/<%=patient._id%>">Update</a></td>
                                        </tr> 
                                        <% } %>
                                        <tr> 
                                        <% } else { %>
                                            <td>No Sessions yet </td>
                                        <% } %>
                                        </tr>
                                    </tbody>
                                    <tfoot>
                                        <tr>
                                            <td><strong>Name</strong></td>
                                            <td><strong>Position</strong></td>
                                            <td><strong>Level/Status</strong></td>
                                            <td>Actions</td>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>
                            <div class="row">
                                <div class="col-md-6 align-self-center">
                                    <p id="dataTable_info" class="dataTables_info" role="status" aria-live="polite">Showing 1 to 10 of <a></p>
                                </div>
                                <div class="col-md-6">
                                    <nav class="d-lg-flex justify-content-lg-end dataTables_paginate paging_simple_numbers">
                                        <ul class="pagination">
                                            <li class="page-item disabled"><a class="page-link" href="#" aria-label="Previous"><span aria-hidden="true">«</span></a></li>
                                            <li class="page-item active"><a class="page-link" href="#">1</a></li>
                                            <li class="page-item"><a class="page-link" href="record.html">2</a></li>
                                            <li class="page-item"><a class="page-link" href="record.html">3</a></li>
                                            <li class="page-item"><a class="page-link" href="#" aria-label="Next"><span aria-hidden="true">»</span></a></li>
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
